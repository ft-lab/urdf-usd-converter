<robot name="duplicate_joint_names">
  <link name="BaseLink">
    <visual>
      <origin rpy="0 0 0" xyz="-2 0 0.5"/>
      <geometry>
        <box size="1 1 1"/>
      </geometry>
    </visual>
  </link>

  <link name="link2">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.5"/>
      <geometry>
        <box size="1 1 1"/>
      </geometry>
    </visual>
  </link>

  <link name="link3">
    <visual>
      <origin rpy="0 0 0" xyz="2 0 0.5"/>
      <geometry>
        <box size="1 1 1"/>
      </geometry>
    </visual>
  </link>

  <joint name="JointA" type="fixed">
    <parent link="BaseLink"/>
    <child link="link2" />
  </joint>

  <!-- This is a deliberate mistake.  -->
  <!-- The joint name is the same as the existing joint name. -->
  <joint name="JointA" type="fixed">
    <parent link="link2"/>
    <child link="link3" />
  </joint>
</robot>
